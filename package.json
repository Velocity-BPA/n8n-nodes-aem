{
  "name": "n8n-nodes-aem",
  "version": "1.0.0",
  "description": "A comprehensive n8n community node for Adobe Experience Manager providing 11 resources and 70+ operations for DAM, content fragments, pages, GraphQL, and workflow automation.",
  "license": "BUSL-1.1",
  "author": {
    "name": "Velocity BPA",
    "email": "licensing@velobpa.com",
    "url": "https://velobpa.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Velocity-BPA/n8n-nodes-aem.git"
  },
  "keywords": [
    "n8n",
    "n8n-community-node-package",
    "adobe",
    "aem",
    "experience-manager",
    "content-management",
    "dam",
    "digital-asset-management",
    "content-fragments",
    "graphql",
    "headless-cms",
    "workflow-automation"
  ],
  "main": "dist/nodes/Aem/Aem.node.js",
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/AemApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Aem/Aem.node.js",
      "dist/nodes/Aem/AemTrigger.node.js"
    ]
  },
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier --write \"**/*.ts\"",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^4.0.2",
    "jest": "^29.7.0",
    "n8n-workflow": "*",
    "prettier": "^3.2.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
